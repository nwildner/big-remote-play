# File: big-remote-play/usr/share/big-remote-play/ui/performance_monitor.py, line: 774
# File: big-remote-play/usr/share/big-remote-play/ui/guest_view.py, line: 151
msgid "Disconnected"
msgstr "Desconectado"
# 
